{
  "workbench.iconTheme": "vscode-icons",
  "workbench.tree.indent": 8,
  "editor.renderWhitespace": "selection",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.wordWrapColumn": 140,
  "editor.guides.bracketPairs": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.lineHeight": 1.8,
  "editor.rulers": [120, 140, 180],
  "gitlens.defaultDateLocale": null,
  "git.confirmSync": false,
  "git.autofetch": true,
  "gitlens.defaultGravatarsStyle": "identicon",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\git-bash.exe",
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.mouseWheelZoom": "",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    }
  },
  "search.showLineNumbers": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "typescript.preferences.quoteStyle": "single",
  "vsicons.dontShowNewVersionMessage": true,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "git.openRepositoryInParentFolders": "never",
  "editor.suggest.showStatusBar": true,
  "gitlens.statusBar.alignment": "left",
  "files.autoSave": "onFocusChange",
  "extensions.ignoreRecommendations": true,
  "gitlens.views.commits.files.icon": "status",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "css.lint.fontFaceProperties": "ignore",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "eslint.format.enable": true,
  "remoteHub.uncommittedChangesOnEntry": "none",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.logMessagePrefix": "",
  "turboConsoleLog.delimiterInsideMessage": ">",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.logMessageSuffix": "",
  "turboConsoleLog.quote": "`",
  "editor.stickyScroll.enabled": false,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "nxConsole.showNodeVersionOnStartup": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "explorer.confirmPasteNative": false,
  "explorer.confirmDelete": false,
  "vscode-angular-html.angularAnimationTriggerPrefix": "#D6D6DD",
  "vscode-angular-html.angularBindingAttributeDelimiter": "#D6D6DD",
  "vscode-angular-html.angularExpression": "#D6D6DD",
  "vscode-angular-html.angularPrefixedAttributesRefPrefix": "#D6D6DD",
  "vscode-angular-html.angularSyntaxSugarAttributesPrefix": "#D6D6DD",
  "vscode-angular-html.angularTemplateVariablePrefix": "#D6D6DD",
  "vscode-angular-html.colorCustomizations": true,
  "vscode-angular-html.dtdDoctypeQuantifier": "#D6D6DD",
  "vscode-angular-html.xmlAttributeNamespaceDivider": "#D6D6DD",
  "vscode-angular-html.xmlTagNamespaceDivider": "#D6D6DD",
  "vscode-angular-html.controlFlowPrefix": "#D6D6DD",
  "vim.leader": "space",
  "vim.smartRelativeLine": true,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.surround": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["K"],
      "after": ["g", "h"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["leader", "c", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["leader", "space"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "s", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["leader", "s", "S"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["leader", "n", "u"],
      "commands": ["extension.switchTs"]
    },
    {
      "before": ["leader", "n", "o"],
      "commands": ["extension.switchTemplate"]
    },
    {
      "before": ["leader", "n", "i"],
      "commands": ["extension.switchStyle"]
    },
    {
      "before": ["leader", "n", "p"],
      "commands": ["extension.switchSpec"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "c", "a"],
      "commands": ["editor.action.codeAction"]
    },
    {
      "before": ["leader", "c", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["leader", "s", "k"],
      "commands": ["workbench.action.openGlobalKeybindings"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "b", "o"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<leader>", "g", "h", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "c", "o"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<C-s>"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<leader>", "g", "h", "p"],
      "commands": ["editor.action.dirtydiff.next"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["workbench.action.findInFiles"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "g", "h", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<C-s>"],
      "commands": [":w"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-s>"],
      "commands": [":w"]
    }
  ],
  "vim.commandLineModeKeyBindingsNonRecursive": [
    {
      "before": ["w"],
      "after": "wa"
    }
  ],
  "vim.handleKeys": {
    "<C-k>": false,
    "<C-s>": false
  },
  "editor.formatOnSave": false,
